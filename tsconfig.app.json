{
	"extends": "@vue/tsconfig/tsconfig.web.json",
	"include": ["env.d.ts", "src/**/*", "src/**/*.vue", "src/enum", "electron/**/*.ts"],
	"exclude": ["src/**/__tests__/*"],
	"compilerOptions": {
		"ignoreDeprecations": "5.0",
		"composite": true,
		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"],
			"@ele/*": ["./electron/*"]
		}
	}
}
